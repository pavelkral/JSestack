{
  "name": "JSestack",
  "version": "0.0.1",
  "description": "JSestack devpack and build setup by webpack tools for writing REACT flux pattern application in es6+",
  "private": true,
  "scripts": {
    "min": "uglifyjs build/bundle.js -c -m -o build/bundle.min.js",
    "copy": "cp build/bundle.min.js build/dist/bundle.min.js",
    "dist": "npm run min && npm run copy"
  },
  "dependencies": {
    "debug": "^2.1.3",
    "flux": "^2.1.1",
    "immutable": "^3.7.5",
    "lodash-node": "^3.10.1",
    "react": "^0.13.1",
    "react-router": "^0.13.3"
  },
  "devDependencies": {
    "babel": "^5.0.12",
    "babel-core": "^5.0.12",
    "babel-loader": "^5.3.2",
    "css-loader": "^0.18.0",
    "style-loader": "^0.12.3",
    "uglify-js": "^2.4.24",
    "webpack": "*",
    "webpack-dev-server": "^1.10.1"
  }
}
